{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "async": true,
  "palette": {
    "closer": "#ACB0BE",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "maroon": "#EBA0AC",
    "teal": "#94e2d5",
    "sky": "#89dceb"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:lavender",
          "style": "plain",
          "template": "{{.Name}} ",
          "type": "shell"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "options": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:lavender",
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .RepoName }} {{ .HEAD }} {{ if not (or .Ahead .Behind) }}\u2261{{ end }}{{ if .Ahead }}<#89dceb>\u2191</>{{ end }}<#EBA0AC>{{ if .Behind }}\u2193{{ end }}{{ if .Staging.Changed }}<#89dceb>+</>{{ end }}{{ if or .Working.Modified .Working.Deleted }}*{{ end }}{{ if .Working.Untracked }}?{{ end }}</>"
        }
      ],
      "type": "prompt"
    },
    {
      "newline": true,
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:closer",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:maroon{{ end }}"
          ],
          "template": "\uf105",
          "options": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "type": "status",
    "style": "plain",
    "foreground": "p:closer",
    "foreground_templates": [
      "{{ if gt .Code 0 }}p:maroon{{ end }}"
    ],
    "template": "\uf105 "
  },
  "final_space": true,
  "version": 4
}